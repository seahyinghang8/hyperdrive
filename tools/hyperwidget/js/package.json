{
  "name": "hyperwidget",
  "version": "0.1.0",
  "description": "A Custom Jupyter Widget Library",
  "author": "hyperdrive",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com//hyperwidget.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf lib/ dist/",
    "build:lib": "babel src --out-dir lib --copy-files",
    "watch:lib": "babel src --out-dir lib --copy-files --watch --verbose",
    "build": "babel src --out-dir lib --copy-files && webpack",
    "build:bundle": "webpack",
    "watch:bundle": "webpack --mode development --watch",
    "prepare": "run-s build:*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "run-p watch:*",
    "lint": "eslint src/**/*.js src/**/*.jsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "lodash": "^4.17.15",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-tabs": "^3.1.1",
    "react-tooltip": "^4.2.10",
    "typeface-roboto": "0.0.54"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
  }
}
